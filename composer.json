{
  "name": "cosname/flarum-wp-password",
  "description": "Allow Flarum to check passwords generated by WordPress.",
  "type": "flarum-extension",
  "keywords": ["wordpress", "password", "hashing"],
  "homepage": "https://github.com/mikemclin/laravel-wp-password",
  "license": "MIT",
  "authors": [
      {
          "name": "Yixuan Qiu",
          "email": "yixuan.qiu@cos.name"
      }
  ],
  "require": {
    "flarum/core": "^0.1.0-beta.6",
    "hautelook/phpass": "^1.1.0"
  },
  "autoload": {
      "psr-4": {
          "Cosname\\": "src/"
      }
  },
  "extra": {
      "flarum-extension": {
          "title": "WP Password",
          "icon": {
              "name": "wordpress",
              "backgroundColor": "#ddd",
              "color": "#333"
          }
      }
  }
}
